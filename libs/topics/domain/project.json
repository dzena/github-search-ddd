{
  "name": "topics-domain",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/topics/domain/src",
  "prefix": "topics",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/topics/domain/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/topics/domain/**/*.ts",
          "libs/topics/domain/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:topics", "type:domain-logic"]
}
